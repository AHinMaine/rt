           { name        => '___Approvals',
             description => 'A system-internal queue for the approvals system',
             disabled    => 2, } );

use Scalar::Defer 'lazy';
my $approvals_id = lazy {
    my $queue = RT::Model::Queue->new( current_user => RT->system_user );
    $queue->load_by_cols(name => "___Approvals");
    $queue->id;
};

